from collections import defaultdict

class Solution:
    def twoSum(self, nums: List[int], target: int) -> List[int]:
        indexes = defaultdict(lambda:[])
        
        for index, number in enumerate(nums):
            indexes[number].append(index)
            if(target - number == number):
                if(len(indexes[number]) > 1):
                    return [indexes[number][0], indexes[number][1]]
            else:
                if(len(indexes[target - number]) == 1):
                    return [indexes[number][0], indexes[target - number][0]]